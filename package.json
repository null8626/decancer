{
  "name": "decancer",
  "version": "1.3.0",
  "main": "src/lib.js",
  "typings": "src/typings.d.ts",
  "description": "A Node.js module that removes common confusables from strings written in pure Rust without the use of Regexes.",
  "keywords": [
    "string",
    "unicode",
    "zalgo",
    "rust",
    "confusables",
    "moderation",
    "homoglyph"
  ],
  "author": "null8626",
  "bugs": {
    "url": "https://github.com/null8626/decancer/issues"
  },
  "homepage": "https://github.com/null8626/decancer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/null8626/decancer"
  },
  "types": "src/typings.d.ts",
  "napi": {
    "name": "decancer",
    "package": {
      "name": "@vierofernando/decancer"
    },
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.9.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "prettier": "npx prettier --write src/lib.ts",
    "artifacts": "napi artifacts",
    "build": "npx tsc --project tsconfig.json && napi build --platform --release",
    "build:debug": "npx tsc --project tsconfig.json && napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "echo \"IT WORKS!111\""
  }
}
